{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "safran-cassiopee/php-metar-decoder": "^0.7.0",
        "safran-cassiopee/php-taf-decoder": "^0.1.0",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^4.2.0",
        "symfony/css-selector": "^4.2.0",
        "symfony/dom-crawler": "^4.2.0",
        "symfony/finder": "^4.2.0",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.1.8",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer": "^4.2.0",
        "symfony/templating": "^4.2.0",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.2.0"
    },
    "require-dev": {
        "rexxars/html-validator": "^2.1",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^4.1",
        "symfony/dotenv": "^4.2.0",
        "symfony/maker-bundle": "^1.9",
        "symfony/phpunit-bridge": "4.1.*",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install ./": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "deploy": [
            "sh ./deploy.sh"
        ],
        "test": [
            "bin/phpunit --testsuite Test --coverage-text"
        ],
        "validate-html": [
            "bin/phpunit --testsuite Validate"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
