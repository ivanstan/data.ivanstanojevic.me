{% extends 'pages/api/base.html.twig' %}

{% block title %}{{ title }} API Documentation | {{ parent() }}{% endblock %}

{% block meta %}
    {% if description is not null %}
        <meta name="description" content="{{ description }}">
    {% endif %}
{% endblock %}

{% block content %}
    <!--suppress HtmlUnknownTag, HtmlUnknownTag -->
    <redoc spec-url="{{ path('app_api_docs_json', {name: name}) }}"></redoc>
{% endblock %}
