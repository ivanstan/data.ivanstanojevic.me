<!doctype html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}OpenData{% endblock %}</title>
    {% block head %}
        <link rel="stylesheet" href="{{ asset('assets/build/app.css') }}">
    {% endblock %}
</head>
<body>
<nav class="navbar bg-primary">
    <a class="navbar-brand" href="{{ path('app_data_index') }}">OpenData</a>
</nav>
{% block header %}{% endblock %}
{% block featured %}{% endblock %}
{% block content %}{% endblock %}

<footer class="bg-primary">
    <div class="footer-copyright text-center py-3">Copyright © {{ "now"|date('Y') }}
        <a href="https://ivanstanojevic.me/"> Ivan Stanojević</a>.
    </div>
</footer>

{% block javascripts %}
    {% include 'components/google-analytics.html.twig' %}
{% endblock %}
</body>
</html>
