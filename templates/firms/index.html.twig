{% extends 'base.html.twig' %}

{% block content %}
    <div id="firms" data-api-key="{{ api_key }}" style="width: 100%; height: 400px"></div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="//maps.googleapis.com/maps/api/js?key={{ api_key }}&callback=renderFirms"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/heatmap.js/2.0.2/heatmap.min.js"></script>
    <script src="{{ asset('assets/js/gmaps-heatmap.js') }}"></script>
{% endblock %}
