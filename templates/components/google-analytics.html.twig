{% if google_analytics_key is defined and google_analytics_key %}
    <script async src="//www.googletagmanager.com/gtag/js?id=UA-37623519-7"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', '{{ google_analytics_key }}');
    </script>
{% endif %}
